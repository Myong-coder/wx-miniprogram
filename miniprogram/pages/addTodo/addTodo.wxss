.container{
  height: 50vh;
  width: auto;
  background-size: contain;
  background-repeat: no-repeat;
  background-image: url(data:image/png;base64,);
}

.addTask{
  width: 600rpx;
  height: 250rpx;
  margin: 0 auto;
  flex-direction: column;
}
/* flex布局只对子元素产生影响 */
.addTask>view{
  flex: 1;
  display: flex;
  /* background-color: green; */
  /* border-bottom: 1rpx solid cyan; */
  margin: 20rpx 0;
}

.tasks{
  border-bottom: 1px solid #e0e0e0;
  flex: 1;
  margin-left: 20rpx;
}

.status radio-group{
  display: flex;
}

button{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 15rpx;
}